    dcterms:isPartOf <https://placeholder.com/catalogue/kn> ;
    dcterms:license <http://purl.org/NET/rdflicense/cc-by4.0> ;
    dcterms:publisher <https://linked.data.gov.au/org/gsq> ;
    dcterms:spatial
